rd Checks.</p>\n</div>","favorite":false,"reference":"","weight":0,"cost":"13","quantity":1,"equiped":true,"modifiers":{"cfoykgsbcatezy8lf5fg":{"type":"impactArmor","formula":"2","flavor":"","isActive":true,"conditions":{"focus":""},"key":"cfoykgsbcatezy8lf5fg","ftype":"nodice","valid":true,"itemId":"7egQbnMLfwJEuADZ","itemName":"Vac Suit"},"xp3wprge7yqwnee2zewd":{"type":"ballisticArmor","formula":"2","flavor":"","isActive":true,"conditions":{"focus":""},"key":"xp3wprge7yqwnee2zewd","ftype":"nodice","valid":true,"itemId":"7egQbnMLfwJEuADZ","itemName":"Vac Suit"},"v5efjxu0wurjvr1z8el8":{"type":"armorPenalty","formula":"1","flavor":"","isActive":true,"conditions":{"focus":""},"key":"v5efjxu0wurjvr1z8el8","ftype":"nodice","valid":true,"itemId":"7egQbnMLfwJEuADZ","itemName":"Vac Suit"},"3nloz3dus3xszgrxao8t":{"type":"impactArmor","formula":"2","flavor":"","isActive":true,"conditions":{"focus":""},"key":"3nloz3dus3xszgrxao8t","ftype":"nodice","valid":true,"itemId":"7egQbnMLfwJEuADZ","itemName":"Vac Suit"},"98gy3ksc9yegisg7u9gq":{"type":"ballisticArmor","formula":"2","flavor":"","isActive":true,"conditions":{"focus":""},"key":"98gy3ksc9yegisg7u9gq","ftype":"nodice","valid":true,"itemId":"7egQbnMLfwJEuADZ","itemName":"Vac Suit"},"osfxm1ivllejzgkppxis":{"type":"armorPenalty","formula":"1","flavor":"","isActive":true,"conditions":{"focus":""},"key":"osfxm1ivllejzgkppxis","ftype":"nodice","valid":true,"itemId":"7egQbnMLfwJEuADZ","itemName":"Vac Suit"}},"itemMods":{"itemDamage":{"value":0,"isActive":false,"selected":false},"testMod":{"value":0,"isActive":false,"selected":false},"attackMod":{"value":0,"isActive":false,"selected":false},"actorDamage":{"value":0,"isActive":false,"selected":false},"itemActivation":{"value":0,"isActive":false,"selected":false},"focus":{"name":"","value":0,"isActive":false,"selected":false},"defense":{"value":0,"isActive":false,"selected":false},"impactArmor":{"value":2,"isActive":true,"selected":true},"ballisticArmor":{"value":2,"isActive":true,"se