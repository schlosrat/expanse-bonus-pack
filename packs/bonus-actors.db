iv>\n<p>Light body armor is made from modern anti-ballistic materials intended to stop bullets and other weapons from penetrating to minimize damage. It is similar in weight to a light jacket or lined jumpsuit, and thus fairly easy to wear without drawing a great deal of notice.</p>\n</div>\n</div>\n</div>\n</div>","favorite":false,"reference":"","weight":0,"cost":"12","quantity":1,"equiped":false,"modifiers":{"rpvu5xusitich19ycgxn":{"type":"impactArmor","formula":"2","flavor":"","isActive":true,"conditions":{"focus":""},"key":"rpvu5xusitich19ycgxn","ftype":"nodice","valid":true,"itemId":"4GYf7wjRgPey4i0c","itemName":"Light Body Armor"},"ybm79y1gxmjnah2cylyo":{"type":"ballisticArmor","formula":"2","flavor":"","isActive":true,"conditions":{"focus":""},"key":"ybm79y1gxmjnah2cylyo","ftype":"nodice","valid":true,"itemId":"4GYf7wjRgPey4i0c","itemName":"Light Body Armor"},"3zohhz4o0qffmba56cmh":{"type":"armorPenalty","formula":"1","flavor":"","isActive":true,"conditions":{"focus":""},"key":"3zohhz4o0qffmba56cmh","ftype":"nodice","valid":true,"itemId":"4GYf7wjRgPey4i0c","itemName":"Light Body Armor"},"xya8ckgo9ljmq4y57kho":{"type":"dex","formula":"null","flavor":"","isActive":false,"conditions":{"focus":""},"key":"xya8ckgo9ljmq4y57kho","ftype":"number","valid":true,"itemId":"4GYf7wjRgPey4i0c","itemName":"Light Body Armor"}},"itemMods":{"itemDamage":{"value":0,"isActive":false,"selected":false},"testMod":{"value":0,"isActive":false,"selected":false},"attackMod":{"value":0,"isActive":false,"selected":false},"actorDamage":{"value":0,"isActive":false,"selected":false},"itemActivation":{"value":0,"isActive":false,"selected":false},"focus":{"name":"","value":0,"isActive":false,"selected":false},"defense":{"value":0,"isActive":false,"selected":false},"impactArmor":{"value":2,"isActive":true,"selected":true},"ballisticArmor":{"value":2,"isActive":true,"selected":true},"toughness":{"value":0,"isActive":false,"selected":false},"speed":{"value":0,"isActive":false,"selected":false},"a