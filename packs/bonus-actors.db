ffects":[],"folder":null,"permission":{"default":0}},{"_id":"alMZqcpjUL8BBMcr","name":"Vicious Blow","type":"stunts","data":{"shortDesc":"","longDesc":"<p>Inflict an extra 1d6 damage on this attack</p>","favorite":false,"reference":"","stuntPoints":2,"stuntPointsMax":1,"costType":"fixed","type":""},"sort":1500000,"flags":{"core":{"sourceId":"Item.XIynb4VvE36sOj4t"}},"img":"systems/age-system/resources/imgs/item-icon/split-cross.svg","effects":[],"folder":null,"permission":{"default":0}},{"_id":"3rUarMSWQdZOoAQ8","name":"Carousing","type":"talent","data":{"shortDesc":"Novice","longDesc":"<p class=\"p1\" style=\"margin-top: 0px; margin-bottom: 0px; font-variant-numeric: normal; font-variant-east-asian: normal; font-stretch: normal; font-size: 13px; line-height: normal; font-family: Helvetica; -webkit-text-stroke-color: #000000;\"><span class=\"s1\" style=\"font-kerning: none; background-color: #ffffff;\"><strong>REQUIREMENTS</strong>: Communication and Constitution 1 or higher</span></p>\n<p class=\"p1\" style=\"margin-top: 0px; margin-bottom: 0px; font-variant-numeric: normal; font-variant-east-asian: normal; font-stretch: normal; font-size: 13px; line-height: normal; font-family: Helvetica; -webkit-text-stroke-color: #000000;\"><span class=\"s1\" style=\"font-kerning: none; background-color: #ffffff;\">You take your fun seriously, and use it to good effect.</span></p>\n<p class=\"p2\" style=\"margin-top: 0px; margin-bottom: 0px; font-variant-numeric: normal; font-variant-east-asian: normal; font-stretch: normal; font-size: 13px; line-height: normal; font-family: Helvetica; -webkit-text-stroke-color: #000000;\"><span class=\"s2\" style=\"font-variant-numeric: normal; font-variant-east-asian: normal; font-stretch: normal; font-size: 11.5px; line-height: normal; font-kerning: none; background-color: #ffffff;\"><strong>NOVICE</strong>: </span><span class=\"s1\" style=\"font-kerning: none; background-color: #ffffff;\">You can drink other people under the table. When making Constitution (Tolerance) tests as part of an advanced test, you gain a +1 bonus to the result of each Drama Die.</span></p>\n<p class=\"p2\" style=\"margin-top: 0px; margin-bottom: 0px; font-variant-numeric: normal; font-variant-east-asian: normal; font-stretch: normal; font-size: 13px; line-height: normal; font-family: Helvetica; -webkit-text-stroke-color: #000000;\"><span class=\"s2\" style=\"font-variant-numeric: normal; font-variant-east-asian: normal; font-stretch: normal; font-size: 11.5px; line-height: normal; font-kerning: none; background-color: #ffffff;\"><strong>EXPERT</strong>: </span><span class=\"s1\" style=\"font-kerning: none; background-color: #ffffff;\">Choose one of the following focuses: Communication (Seduction) or Perception (Empathy). If you fail a test with your chosen focus, you can re-roll it but must keep the result of the second roll.</span></p>\n<p class=\"p3\" style=\"margin-top: 0px; margin-bottom: 0px; font-variant-numeric: normal; font-variant-east-asian: normal; font-stretch: normal; font-size: 13px; line-height: normal; font-family: Helvetica; -webkit-text-stroke-color: #000000; background-color: #ffffff;\"><span class=\"s3\" style=\"font-variant-numeric: normal; font-variant-east-asian: normal; font-stretch: normal; font-size: 11.5px; line-height: normal; font-kerning: none;\"><strong>MASTER</strong>:<span class=\"Apple-converted-space\">&nbsp;</span></span><span class=\"s4\" style=\"font-kerning: none;\"> When you&rsquo;re having a good time, you&rsquo;re invincible! Or so you think. Choose the Benefit of the Doubt or Flirt stunt. You can perform that stunt for 1 SP less than its normal cost.</span></p>","favorite":false,"reference":"","modifiers":{},"itemMods":{"itemDamage":{"value":0,"isActive":false,"selected":false},"testMod":{"value":0,"isActive":false,"selected":false},"attackMod":{"value":0,"isActive":false,"selected":false},"actorDamage":{"value":0,"isActive":false,"selected":false},"itemActivation":{"value":0,"isActiv